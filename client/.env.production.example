### Env variables required for building frontend code - update value as needed
NODE_PATH=src/
REACT_APP_DEFAULT_LANDING=1
REACT_APP_HTTP_PROTOCOL=https://
REACT_APP_SERVER_HOST=localhost
REACT_APP_SERVER_PORT=8000
REACT_APP_WS_PROTOCOL=wss://

# By default, only LDAP mode is enabled for prod
REACT_APP_ENABLE_LDAP_SYNC=1
REACT_APP_ENABLE_NEXT_BASE_USE=0

# This number will update automatically each time ./bin/build_client is executed
REACT_APP_VERSION_NUMBER=0.0.0
